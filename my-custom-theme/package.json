{
  "name": "shipshipship-template-default",
  "description": "A beautiful changelog template for ShipShipShip",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": "ShipShipShip",
  "keywords": [
    "shipshipship",
    "theme",
    "changelog",
    "clear",
    "minimal"
  ],
  "scripts": {
    "dev": "vite dev",
    "dev:host": "vite dev --host",
    "dev:proxy": "node dev-proxy.js",
    "build": "svelte-kit sync && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "clean": "rm -rf .svelte-kit build dist",
    "env:example": "cp .env.example .env.local",
    "config:show": "node -e \"console.log('VITE_ADMIN_API_URL:', process.env.VITE_ADMIN_API_URL || '(not set)');\""
  },
  "dependencies": {
    "@iconify-json/circle-flags": "^1.2.10",
    "@iconify-json/fluent-emoji-flat": "^1.2.5",
    "@iconify/svelte": "^5.1.0",
    "@tiptap/core": "^3.1.0",
    "@tiptap/extension-image": "^3.1.0",
    "@tiptap/extension-link": "^3.1.0",
    "@tiptap/extension-table": "^3.1.0",
    "@tiptap/extension-table-cell": "^3.1.0",
    "@tiptap/extension-table-header": "^3.1.0",
    "@tiptap/extension-table-row": "^3.1.0",
    "@tiptap/pm": "^3.1.0",
    "@tiptap/starter-kit": "^3.1.0",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.447.0",
    "shadcn-svelte": "^0.14.3",
    "svelte-sonner": "^0.3.27",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.410",
    "@inlang/paraglide-js": "2.5.0",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "svelte": "^5.44.0",
    "svelte-check": "^4.0.2",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2",
    "vite": "^6.4.1"
  }
}
